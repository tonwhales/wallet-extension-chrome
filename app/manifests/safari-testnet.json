{
    "name": "Ton Dev extension",
    "description": "Ton Dev for Web extension",
    "version": "0.6.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Ton Dev for Web",
        "default_popup": "index.html"
    },
    "icons": {
           "64": "dev_128x128_webstore.png"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": ["worker.js"]
    },
    "incognito": "spanning",
    "web_accessible_resources": [ "browser.js" ],
    "content_scripts": [
        {
            "matches": [
                "file://*/*",
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "inject.js"
            ]
        }
    ]
}
